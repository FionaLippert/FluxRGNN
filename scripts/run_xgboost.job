#!/bin/bash

#SBATCH --partition=genoa
#SBATCH --nodes=1
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=4
#SBATCH --job-name=xgboost
#SBATCH --time=5:00:00
#SBATCH --output=slurm_output_%A_%a.out

source activate fluxrgnn

cd $HOME/FluxRGNN/scripts

which python

nvidia-smi

OVERRIDES=$1

echo $OVERRIDES

srun python run_xgboost.py $OVERRIDES

